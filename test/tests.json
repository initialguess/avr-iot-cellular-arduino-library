{
  "debug_modem": {
    "enabled": true,
    "tests": [
      {
        "command": "AT\r",
        "expectation": "AT"
      },
      {
        "expectation": "OK"
      },
      {
        "command": "AT+CEREG?\r",
        "expectation": "AT\\+CEREG\\?"
      },
      {
        "expectation": "\\+CEREG: 5,0"
      },
      {
        "expectation": "\r\n"
      },
      {
        "expectation": "OK"
      }
    ]
  },
  "extract_certificates": {
    "enabled": true,
    "tests": [
      {
        "expectation": "\\[INFO\\] Initialized ECC"
      },
      {
        "repeat": 3,
        "expectation": "\r\n"
      },
      {
        "expectation": "\\[INFO\\] Printing signing certificate..."
      },
      {
        "expectation": "\r\n"
      },
      {
        "expectation": "-----BEGIN CERTIFICATE-----"
      },
      {
        "repeat": 11,
        "expectation": "([^\\n]+)"
      },
      {
        "expectation": "-----END CERTIFICATE-----"
      },
      {
        "repeat": 2,
        "expectation": "\r\n"
      },
      {
        "expectation": "\\[INFO\\] Printing device certificate..."
      },
      {
        "expectation": "\r\n"
      },
      {
        "expectation": "-----BEGIN CERTIFICATE-----"
      },
      {
        "repeat": 11,
        "expectation": "([^\\n]+)"
      },
      {
        "expectation": "-----END CERTIFICATE-----"
      }
    ]
  },
